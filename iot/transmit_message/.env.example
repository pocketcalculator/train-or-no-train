# IoT Message Transmission Configuration
# Copy this file to .env and fill in your actual values

# Azure IoT Hub Device Connection String
# This is specific to your device, get it from Azure IoT Hub
AZURE_IOT_CONNECTION_STRING="HostName=YOUR_IOT_HUB.azure-devices.net;DeviceId=YOUR_DEVICE_ID;SharedAccessKey=YOUR_SHARED_ACCESS_KEY"

# Message settings
MESSAGE_INTERVAL_SECONDS=30
ENABLE_TELEMETRY=true
DEBUG_MODE=false